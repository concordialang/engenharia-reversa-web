{
    "manifest_version": 2,
    "name": "TCC",
    "version": "0.1.0",
    "background": {
        "page": "/dist/background.html"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["/dist/content.js"]
        }
    ],
    "web_accessible_resources": [
       "/node_modules/graph-data-structure/index.js",
       "/dist/bundle.js"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": ["tabs", "<all_urls>"]
}